{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "customlang",
      "camelcase": "Customlang",
      "title": "Customlang",
      "scope": "source.customlang",
      "file-types": ["maxi_customlang"],
      "injection-regex": "^customlang$",
      "class-name": "TreeSitterCustomlang"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Customlang grammar for tree-sitter",
    "authors": [
      {
        "name": "Maxi Myoga"
      }
    ],
    "links": {
      "repository": "https://github.com/CatMaxiMyoga/rust-customlang/tree/master/tree-sitter"
    }
  },
  "bindings": {
    "c": false,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
